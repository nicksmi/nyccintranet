<?php

function nycc_terms_conditions_install() {
      variable_set('termsandconditions_pageTitle', 'Marketplace terms and conditions');
	  variable_set('termsandconditions_text', 'Please update your terms and conditions');
}
 
function nycc_terms_conditions_uninstall() {
      variable_del('termsandconditions_pageTitle');
	  variable_del('termsandconditions_text');
}